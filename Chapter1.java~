public class Chapter1{
    
    //Make a binary representation of a number N
    public static String binaryRepPosInt(int N){
        String s = "";
        for(int n = N; n > 0; n /= 2)
            s = (n % 2) + s;
        
        System.out.println(s);
        return s;
    }
    
